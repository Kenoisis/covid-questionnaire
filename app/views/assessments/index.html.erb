<p id="notice"><%= notice %></p>

<h1>Assessments</h1>

<table>
  <thead>
    <tr>
      <th>Priority</th>
      <th>Name</th>
      <th>Last name</th>
      <th>Birthdate</th>
      <th>City</th>
      <th>Country</th>
      <th>Gender</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assessments.each do |assessment| %>
      <tr>
        <td><%= assessment.priority %></td>
        <td><%= assessment.name %></td>
        <td><%= assessment.last_name %></td>
        <td><%= assessment.birthdate %></td>
        <td><%= assessment.city %></td>
        <td><%= assessment.country %></td>
        <td><%= assessment.gender %></td>
        <td><%= link_to 'Show', assessment %></td>
        <td><%= link_to 'Edit', edit_assessment_path(assessment) %></td>
        <td><%= link_to 'Destroy', assessment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assessment', new_assessment_path %>
